<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SignupProcess</servlet-name>
        <servlet-class>SignupProcess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginProcess</servlet-name>
        <servlet-class>LoginProcess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeActivate</servlet-name>
        <servlet-class>DeActivate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayDetails</servlet-name>
        <servlet-class>DisplayDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateDetails</servlet-name>
        <servlet-class>UpdateDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchContacts</servlet-name>
        <servlet-class>FetchContacts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilterContacts</servlet-name>
        <servlet-class>FilterContacts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditContacts</servlet-name>
        <servlet-class>EditContacts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddContacts</servlet-name>
        <servlet-class>AddContacts</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignupProcess</servlet-name>
        <url-pattern>/signupProcessor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginProcess</servlet-name>
        <url-pattern>/loginProcessor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeActivate</servlet-name>
        <url-pattern>/deactivate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayDetails</servlet-name>
        <url-pattern>/DisplayDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateDetails</servlet-name>
        <url-pattern>/UpdateDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FetchContacts</servlet-name>
        <url-pattern>/FetchDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilterContacts</servlet-name>
        <url-pattern>/FilterContacts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditContacts</servlet-name>
        <url-pattern>/EditContacts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddContacts</servlet-name>
        <url-pattern>/AddContacts</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
